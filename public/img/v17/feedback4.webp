<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 300" style="background: #f8fafc;">
  <rect width="400" height="300" fill="#f8fafc"/>
  
  <!-- Card Background -->
  <rect x="20" y="20" width="360" height="260" rx="12" fill="white" stroke="#e2e8f0" stroke-width="2"/>
  
  <!-- Header -->
  <rect x="20" y="20" width="360" height="60" rx="12" fill="url(#pinkGradient)"/>
  <text x="200" y="45" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white">
    Client Feedback #4
  </text>
  <text x="200" y="65" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="rgba(255,255,255,0.9)">
    Boutique Owner - Florida
  </text>
  
  <!-- Stars -->
  <g transform="translate(140, 100)">
    <text font-size="24" fill="#fbbf24">⭐⭐⭐⭐⭐</text>
  </g>
  
  <!-- Quote -->
  <text x="200" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#1e293b" font-weight="600">
    "Working with this brand has elevated"
  </text>
  <text x="200" y="160" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#1e293b">
    "our boutique's reputation. The crystal"
  </text>
  <text x="200" y="180" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#1e293b">
    "and brass pieces are absolutely"
  </text>
  <text x="200" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#1e293b">
    "stunning and well-crafted."
  </text>
  
  <!-- Signature -->
  <text x="200" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#ec4899">
    - Amanda K., Bella Bridal FL
  </text>
  
  <defs>
    <linearGradient id="pinkGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#ec4899"/>
      <stop offset="100%" stop-color="#be185d"/>
    </linearGradient>
  </defs>
</svg>